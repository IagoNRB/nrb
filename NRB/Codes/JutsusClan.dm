				if(M.party&&M.partysensei==O.partysensei)return
				var/damage = src.nin/4 - M.Resistencia/2.5
				if(M.PK == 0)
				///	usr<<"<font size=1>Non PK NON PK!"
					return
				if(M.health<=0)
					M.health=0
					M.Death(O)
				if(damage >= 1)
				{
					if(O.mssasuke||O.implantoumssasuke)
						M.health -= damage*1.1
						F_damage(M, damage, "#ff0000")
					else
						M.health -= damage
						F_damage(M, damage, "#ff0000")
					view(M) << "[M] recebeu [damage] de dano por entrar no amaterasu deixado pelo jutsu do adversario!!"
					if(M.health<=0)
						M.health=0
						M.Death(O)
				}
		spawn(5)
		src.burn()

