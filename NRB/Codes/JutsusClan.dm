	verb
		BeeSabakuSousou()
			set category = "Jutsus"
			set name = "Inseto Sousou"
			usr.Handseals()
			if(!usr.handseals)
				return
			if(usr.froze)
				usr<<"Você está paralisado"
				return
			if(usr.Frozen)
				usr<<"Você está paralisado"
				return
			if(usr.caught)
				usr<<"Você foi capturado"
				return
			if(usr.captured)
				usr<<"Você foi capturado"
				return
			if(usr.resting)
				usr<<"Não enquanto descansa"
				return
			if(usr.meditating)
				usr<<"Não enquanto medita"

			for(var/mob/M in oview(10,usr))
				if(M.party&&M.partysensei==usr.partysensei)return
				if(usr.sphere)
					return
				if(src.Kaiten)
					return
				if(usr.PK==0)
					usr<<"Zona NON-PK!!!"
					return
				if(M.PK==0)
					usr<<"Zona NON-PK!!!"
					return
				if(M.counter)
					usr<<"Ele copia e o reflete de volta para você."
					M<<"Você copiou e o refletiu."
					usr.health -= M.nin/5
					Death(usr)
					return
				if(usr.chakra<=1000)
					usr<<"Não tem chakra o bastante."
					return
				if(M.captured)
					usr.EsquivaSharingan(M)
					if(M.Esquivou)
						M.Esquivou=0
						return
					usr.cast=1
					usr.froze=1
					var/damage=usr.nin/4.25 + usr.tai/4.25 + usr.gen/4.25 + usr.shurikenskill/4.25 - M.Resistencia/2
					if(damage<=0)damage=0
					view(usr)<<"Inseto Sousou!"
					flick("sousou",M)
					M.overlays-='BeeArmor.dmi'
					M.overlays-='BeeArmor.dmi'
					usr.Chakragain()
					M.overlays-='BeeArmor.dmi'
					M.health-=damage
					F_damage(M, damage, "#ff0000")
					sleep(30)
					usr.froze=0
					M.captured=0
					usr.cast=0
					if(M.health<=0)
						M.Death(usr)
						return
				else
					return
