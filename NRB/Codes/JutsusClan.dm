	verb
		Kuuin()
			set category = "Jutsus"
			set name = "Chakra Kuuin no Jutsu"
			if(usr.cast)
				return
			if(usr.froze)
				usr<<"Você está paralisado"
				return
			if(usr.resting)
				usr<<"Não enquanto descansa"
				return
			if(usr.meditating)
				usr<<"Não enquanto medita"
				return
			if(usr.Frozen)
				usr<<"Você está paralisado"
				return
			if(usr.caught)
				usr<<"Você foi capturado"
				return
			if(usr.captured)
				usr<<"Você foi capturado"
				return
			if(usr.chakra >= usr.Mchakra)
				return
			else
				for(var/mob/M in oview(3))
					if(M.party&&M.partysensei==usr.partysensei)return
					if(!M.client)
						usr<<"Você não pode usar isso em NPCs."
						return
					if(M.sapo)
						usr<<"Você absorveu chakra natural e ficou paralisado"
						usr.froze = 1
						usr.move=0
						sleep(100)
						usr.froze=0
						usr.move=1
						return
					if(M.fantasma)
						usr<<"Vc não pode tocá-lo"
						return
					usr<<"Você começou a absorver o chakra de [M]"
					usr.overlays+='chakra.dmi'
					var/damage = usr.nin/4.5 + usr.gen/4.5 + usr.tai/4.5 + usr.shurikenskill/4.5 - M.Resistencia/2
					if(damage<=0)damage=0
					if(damage >= 1)
						if(usr.seals < 100)
							if(damage>=30000)
								damage=30000
								M.chakra -= damage
							else
								M.chakra -= damage
						if(usr.seals>=100)
							if(damage>=50000)
								damage=50000
								M.chakra -= damage
							else
								M.chakra-= damage
						F_damage(M, damage, "#800080")
						usr.chakra += damage
						F_damage(usr, damage, "#0000ff")
						M<<"[usr] está absorvendo você."
						usr.Death(M)
						usr.overlays-='chakra.dmi'


