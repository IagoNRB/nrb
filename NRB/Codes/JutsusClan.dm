				usr<<"Ative seu sharingan primeiro"
				return
			if(usr.resting)
				usr<<"Não enquanto descansa"
				return
			if(usr.meditating)
				usr<<"Não enquanto medita"
				return
			if(usr.fcopy==0)
				if(usr.cdcopy)return
				usr.RandomChakra01()
				usr.cdcopy=1
				spawn()usr.Cooldown(600,"Fuuin no Copy")
				if(usr.chakra<=0)
					usr<<"Vc não possuia chakra suficiente para esse jutsu. Por isso o jutsu falhou e seu chakra foi drenado a ZERO"
					usr.health=0
					usr.chakra=0
					usr.Death(usr)
					return
				usr.fcopy=1
				usr.overlays += 'SharCopy.dmi'
				sleep(50)
				usr.fcopy=0
				usr.overlays -= 'SharCopy.dmi'
			else
				usr<<"Ja esta ativado."

