mob/Sword
	verb
		ChakraA(mob/M in oview(3))
			set category = "TaiJutsu"
			set name = "Absorver Chakra"
			if(usr.cdabkisame)
				usr << "<b>Cooldown:</b> Aguarde para usar novamente o jutsu <b>Absorver Chakra</b>!"
				return
			if(usr.froze)
				usr<<"Você está paralisado"
				return
			if(usr.resting)
				usr<<"Não enquanto descansa"
				return
			if(usr.meditating)
				usr<<"Não enquanto medita"
				return
			if(usr.Frozen)
				usr<<"Você está paralisado"
				return
			if(usr.caught)
				usr<<"Você foi capturado"
				return
			if(M.sapo)
				usr<<"Você absorveu chakra natural e ficou paralisado"
				usr.Frozen = 1
				sleep(30)
				usr.Frozen=0
				return
			if(M.fantasma)
				usr<<"Vc não pode tocá-lo"
				return
			if(usr.captured)
				usr<<"Você foi capturado"
				return
			else
				if(usr.cdabkisame)return
				usr.cdabkisame=1
				spawn()usr.Cooldown(100,"Absorver Chakra")
				usr<<"You start to absorb [M]"
				usr.overlays+='chakra.dmi'
				var/damage = usr.nin/10 + usr.gen/10 + usr.tai/10 + usr.shurikenskill/10 - M.Resistencia/1.5
				if(damage<=0)damage=0
				if(damage >= 1)
					M.chakra -= damage
					F_damage(M, damage, "#800080")
					usr.chakra += damage
					F_damage(usr, damage, "#0000ff")
					M<<"[usr] Está absorvendo você"
					usr.Death(M)
					usr.overlays-='chakra.dmi'

