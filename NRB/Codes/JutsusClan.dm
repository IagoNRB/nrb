				usr<<"Não enquanto medita"
				return
			else
				for(var/mob/M in oview(3))
					if(M.antigen)
						usr<<"Ele está protegido contra genjutsus"
						return
					if(M.gkai&&M.gen>=usr.gen)
						usr<<"Ele percebeu seu genjutsu e se livrou"
						M<<"Seu Kai livrou-lhe de um genjutsu"
						return
